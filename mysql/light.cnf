[mysqld]
performance_schema = off
key_buffer_size = 32M
tmp_table_size = 4M
innodb_buffer_pool_size = 32M
innodb_log_buffer_size = 4M
max_connections = 25
sort_buffer_size = 1M
read_buffer_size = 512K
read_rnd_buffer_size = 1M
join_buffer_size = 256K
thread_stack = 256K

sql_mode = "STRICT_TRANS_TABLES,NO_ENGINE_SUBSTITUTION"